{
  "name": "react-contactcomponents",
  "version": "1.0.0",
  "description": "",
  "main": "script.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "onchange -d 100 sass/styles.scss -- npm run compiler",
    "compiler": "node-sass sass/styles.scss -o css/",
    "start:script": "babel script.js --watch --out-file script-compiled.js",
    "start:app": "babel components/App.js --watch --out-file components/App-compiled.js",
    "start:contact": "babel components/Contact.js --watch --out-file components/Contact-compiled.js",
    "start:contactform": "babel components/ContactForm.js --watch --out-file components/ContactForm-compiled.js",
    "start:contacts": "babel components/Contacts.js --watch --out-file components/Contacts-compiled.js",
    "start": "npm-run-all start:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PioterDz/react-contactcomponents.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PioterDz/react-contactcomponents/issues"
  },
  "homepage": "https://github.com/PioterDz/react-contactcomponents#readme",
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "autoprefixer-cli": "^1.0.0",
    "babel-standalone": "^6.26.0",
    "node-sass": "^4.12.0",
    "onchange": "^6.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest": "^6.24.1"
  }
}
